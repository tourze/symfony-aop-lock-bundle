{
  "name": "tourze/symfony-aop-lock-bundle",
  "description": "Lock for Symfony",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "symfony/framework-bundle": "^6.4",
    "symfony/lock": "^6.4",
    "symfony/http-kernel": "^6.4",
    "twig/twig": "^3.13|^4.0",
    "tourze/symfony-aop-bundle": "~0.0.4"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0",
    "shipmonk/composer-dependency-analyser": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Symfony\\AopLockBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Symfony\\AopLockBundle\\Tests\\": "tests"
    }
  }
}